@model CreateCategoryDto
@{
    ViewData["Title"] = "CreateCategory";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Yeni Kategori Oluştur</h3>

<form method="post">
<div class="form-group">
    <label>Kategori Adı</label>
    <input type="text"class="form-control"asp-for="CategoryName"/>
        <span class="text-danger" asp-validation-for="CategoryName"></span>
</div>
<button class="btn btn-outline-primary"><i class="fa fa-save"></i>Kaydet</button>
</form>