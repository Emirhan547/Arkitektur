@model CreateTeamDto
@{
    ViewData["Title"] = "CreateTeam";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Yeni Ekip Üyesi Oluştur</h3>

<form method="post"enctype="multipart/form-data">
    
    <div class="form-group">
        <label>Ekip Üyesi Profil Fotoğrafı</label>
        <input type="file" asp-for="File" class="form-control" />
        <span class="text-danger" asp-validation-for="File"></span>
    </div>
    <div class="form-group">
        <label>Ad Soyad</label>
        <input type="text" asp-for="NameSurname" class="form-control" />
        <span class="text-danger" asp-validation-for="NameSurname"></span>
    </div>
    <div class="form-group">
        <label>Unvan</label>
        <input type="text" asp-for="Title" class="form-control" />
        <span class="text-danger" asp-validation-for="Title"></span>
    </div>

    <button class="btn btn-outline-primary">Kaydet</button>

</form>